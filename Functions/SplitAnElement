Function SplitAnElement(TexteToSplit As String, Spliter As String, IDElement As Integer)
    
    Splited = Split(TexteToSplit, Spliter)
    Taille = UBound(Splited) - LBound(Splited) + 1
    
    If Taille > 0 Then
        SplitAnElement = Splited(IDElement)
    Else
        SplitAnElement = ""
    End If
    
    
End Function
